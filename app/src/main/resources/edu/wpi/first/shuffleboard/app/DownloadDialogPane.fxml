<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="edu.wpi.first.shuffleboard.app.DownloadDialogController"
      fx:id="root">
    <padding>
        <Insets topRightBottomLeft="8"/>
    </padding>
    <Label text="Downloading update..."/>
    <StackPane prefHeight="30" prefWidth="300">
        <ProgressBar fx:id="progressBar" progress="0" maxWidth="Infinity"/>
        <Label fx:id="label" text="0.00%"/>
    </StackPane>
</VBox>
