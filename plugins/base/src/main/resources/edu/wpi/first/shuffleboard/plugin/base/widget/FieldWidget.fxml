<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.ImageView?>

<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="edu.wpi.first.shuffleboard.plugin.base.widget.FieldWidget"
      fx:id="root"
      spacing="8"  minWidth="192">
    <BorderPane fx:id="pane">
        <ImageView fx:id="backgroundImage" preserveRatio="true" />
        <ImageView fx:id="robot" preserveRatio="true" />
    </BorderPane>
</VBox>